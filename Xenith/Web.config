  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
    <connectionStrings>
        <add name="XenithConnectionString" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\xenith.mdf;Integrated Security=True;User Instance=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
        <urlMappings enabled="true">
          <add url="~/" mappedUrl="~/index.aspx" />
          <add url="~/admin/" mappedUrl="~/admin/index.aspx" />
        </urlMappings>
    </system.web>
</configuration>
